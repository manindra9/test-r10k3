{
  "name": "waveclaw-subscription_manager",
  "version": "3.3.0",
  "author": "JD Powell <waveclaw@hotmail.com>",
  "summary": "Manage Katello or Satellite registrations.",
  "license": "Apache-2.0",
  "source": "https://github.com/waveclaw/puppet-subscription_manager",
  "project_page": "https://github.com/waveclaw/puppet-subscription_manager",
